    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
