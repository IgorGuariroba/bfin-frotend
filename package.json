    "@tanstack/react-query": "^5.17.9",
    "axios": "^1.6.5",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.309.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.3",
