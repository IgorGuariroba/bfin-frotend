  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup:npmrc": "node scripts/setup-npmrc.js",
    "install:all": "node scripts/setup-npmrc.js && npm install",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:storybook\"",
    "dev:vite": "vite",
    "dev:storybook": "storybook dev -p 6006",
